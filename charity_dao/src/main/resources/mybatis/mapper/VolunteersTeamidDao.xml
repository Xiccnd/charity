<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.cqwu.charity_dao.mapper.VolunteersTeamidDao">

    <resultMap id="BaseResultMap" type="net.cqwu.charity_commons.pojo.VolunteersTeamid">
        <!--@Table volunteers_teamid-->
        <result property="id" column="id" jdbcType="INTEGER"/>
        <result property="teamid" column="teamid" jdbcType="INTEGER"/>
        <result property="joinTime" column="joinTime" jdbcType="TIMESTAMP"/>
    </resultMap>

    <!--查询单个-->
    <select id="queryById" resultMap="BaseResultMap">
        select
          id, teamid , joinTime
        from charity.volunteers_teamid
        where id = #{id}
    </select>

    <!--查询指定行数据-->
    <select id="queryAllByLimit" resultMap="BaseResultMap">
        select
          id, teamid, joinTime
        from charity.volunteers_teamid
        limit #{offset}, #{limit}
    </select>

    <!--通过实体作为筛选条件查询-->
    <select id="queryAll" resultMap="BaseResultMap">
        select
          id, teamid, joinTime
        from charity.volunteers_teamid
        <where>
            <if test="id != null">
                and id = #{id}
            </if>
            <if test="teamid != null">
                and teamid = #{teamid}
            </if>
        </where>
    </select>

    <!--新增所有列-->
    <insert id="insert" keyProperty="" useGeneratedKeys="true">
        insert into charity.volunteers_teamid(id, teamid, joinTime)
        values (#{id}, #{teamid},#{joinTime})
    </insert>

    <!--通过主键修改数据-->
    <update id="update">
        update charity.volunteers_teamid
        <set>
            <if test="id != null">
                id = #{id},
            </if>
            <if test="teamid != null">
                teamid = #{teamid},
            </if>
            <if test="joinTime != null">
                joinTime = #{joinTime},
            </if>
        </set>
        where id = #{id}
    </update>

    <!--通过主键删除-->
    <delete id="deleteById">
        delete from charity.volunteers_teamid where id = #{id}
    </delete>

</mapper>